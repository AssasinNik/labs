{
    "name": "elk",
    "config": {
        "connector.class": "io.confluent.connect.elasticsearch.ElasticsearchSinkConnector",
        "tasks.max": "1",
        "topics.regex": "postgres\\.public\\.(?!(student$|student_view_table$)).*",
        "connection.url": "http://elasticsearch:9200",
        "type.name": "_doc",
        "key.ignore": "true",
        "schema.ignore": "true",
        "transforms": "unwrap,castBoolean",
        "transforms.unwrap.type": "io.debezium.transforms.ExtractNewRecordState",
        "transforms.unwrap.drop.tombstones": "false",
        "transforms.unwrap.delete.handling.mode": "rewrite",
        "transforms.castBoolean.type": "org.apache.kafka.connect.transforms.Cast$Value",
        "transforms.castBoolean.spec": "tech_equipment:boolean,default:boolean",
        "behavior.on.malformed.documents": "WARN",
        "behavior.on.null.values": "delete",
        "key.converter": "org.apache.kafka.connect.json.JsonConverter",
        "key.converter.schemas.enable": "false",
        "value.converter": "org.apache.kafka.connect.json.JsonConverter",
        "value.converter.schemas.enable": "false",
        "elastic.version": "7"
    }
}